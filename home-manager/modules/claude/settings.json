{
  "permissions": {
    "allow": [
      "Bash(notify:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr checks:*)",
      "Bash(jj log:*)",
      "Bash(jj-main-branch:*)",
      "Bash(jj-current-branch:*)",
      "Bash(jj-prev-branch:*)",
      "Bash(jj-stacked-branches:*)",
      "Bash(jj-diff-working:*)",
      "Bash(jj-diff-branch:*)",
      "Bash(jj-stacked-stats:*)"
    ]
  },
  "model": "opusplan",
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.message' | xargs -I _ notify \"_\""
          }
        ]
      }
    ]
  },
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  }
}